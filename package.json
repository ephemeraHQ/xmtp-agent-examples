{
  "name": "xmtp-agent-examples",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:examples": "yarn workspaces foreach --all --include \"examples/*\" run build",
    "chat": "tsx cli/chat.tsx",
    "clean": "rimraf node_modules && yarn clean:dbs",
    "clean:dbs": "rimraf *.db3* ||:",
    "dev": "cd examples/xmtp-gm && tsx --watch index.ts",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "gen:keys": "tsx scripts/generateKeys.ts",
    "lint": "eslint .",
    "revoke": "tsx scripts/revokeInstallations.ts",
    "start": "cd examples/xmtp-gm && tsx index.ts",
    "typecheck": "tsc"
  },
  "resolutions": {
    "@xmtp/agent-sdk": "1.1.7"
  },
  "dependencies": {
    "@xmtp/agent-sdk": "1.1.7",
    "dotenv": "^16.4.7",
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.0",
    "@xmtp/content-type-primitives": "^2.0.2",
    "@xmtp/content-type-remote-attachment": "^2.0.2",
    "@xmtp/content-type-wallet-send-calls": "^1.0.1",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "uint8arrays": "^5.1.0"
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=20"
  }
}
