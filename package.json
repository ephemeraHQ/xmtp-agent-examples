{
  "name": "xmtp-agent-examples",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:examples": "yarn workspaces foreach --all --include \"examples/*\" run build",
    "clean": "rimraf node_modules && yarn clean:dbs",
    "clean:dbs": "rimraf *.db3* ||:",
    "dev": "tsx --watch examples/gm/index.ts",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "gen:keys": "tsx scripts/generateKeys.ts",
    "lint": "eslint .",
    "start": "tsx examples/gm/index.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@xmtp/node-sdk": "1.0.2",
    "uint8arrays": "^5.1.0",
    "viem": "^2.22.17"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=20"
  }
}
